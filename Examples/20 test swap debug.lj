/* Test swap function in isolation */

#pragma appname "Swap Debug"
#pragma console on
#pragma optimizecode on

print("=== Testing Swap Function ===\n");

// Initialize two variables
a0 = 50;
a1 = 30;

print("Before swap: a0=", a0, " a1=", a1, "\n");

// Simple swap function
func testswap() {
    temp = a0;
    a0 = a1;
    a1 = temp;
    return 1;
}

// Call swap
result = testswap();

print("After swap: a0=", a0, " a1=", a1, "\n");
print("Result: ", result, "\n");

// Test conditional swap
a0 = 80;
a1 = 20;

print("\nBefore conditional swap: a0=", a0, " a1=", a1, "\n");

func condswap() {
    swapped = 0;
    if (a0 > a1) {
        temp = a0;
        a0 = a1;
        a1 = temp;
        swapped = 1;
    }
    return swapped;
}

result = condswap();

print("After conditional swap: a0=", a0, " a1=", a1, "\n");
print("Swapped: ", result, "\n");

print("\n=== Done ===\n");
