/* Test #pragma ListASM on
   This will output the generated assembly/bytecode to debug output
*/

#pragma appname "ListASM Test"
#pragma console on
#pragma ListASM on

print("=== Testing ListASM Pragma ===\n");
print("Check your debug output to see generated bytecode!\n");

x = 10;
y = 20;
z = x + y;

print("x = ", x, "\n");
print("y = ", y, "\n");
print("z = x + y = ", z, "\n");

print("\nDone! Check debug output for assembly listing.\n");
